pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Taifhoon', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '20', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                sh 'python --version'
                sh 'python myscript.py'
            }
        }
    }
}